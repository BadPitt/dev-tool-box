{
  "name": "DevToolbox",
  "version": "0.1.0",
  "description": "Extension with the most frequently used developer's tools",
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup/index.html",
    "default_title": "Developer's toolbox",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "options_page": "options/index.html",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}